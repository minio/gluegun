            <%- if !@site_map['Plugins'].nil? -%>
              <%- @site_map['Plugins'].each do |categories| -%>
                <%- if categories.key?("Google Analytics") -%>
                  <!-- Google Analytics -->
                  <script>
                    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
                    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
                    ga('create', '<%= categories["Google Analytics"] %>', 'auto');
                    ga('send', 'pageview');
                  </script>
                <%- elsif categories.key?("Algolia") && !categories["Algolia"].nil? -%>
                  <%- if categories["Algolia"].key?("apiKey") && categories["Algolia"].key?("index") -%>
                    <!-- Algolia DocSearch -->
                    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/docsearch.js@2/dist/cdn/docsearch.min.js"></script>
                    <script type="text/javascript">
                      docsearch({
                        apiKey: '<%= categories["Algolia"]["apiKey"] %>',
                        indexName: '<%= categories["Algolia"]["index"] %>',
                        inputSelector: '.search__text'
                      });
                    </script>
                  <%- end -%>
                <%- end -%>
              <%- end -%>
            <%- end -%>
        </footer>
        </section>

        <script src="vendors/jquery/jquery-3.3.1.min.js"></script>
        <script src="vendors/highlight.js/highlight.min.js"></script>
        <script src="js/gluegun.min.js"></script>

            <script>
                $(function() {
                    $(".nav > li > a").on("click",function(e) {
                        e.preventDefault();
                        $("#minio_quickstart_guide").load(this.href);
                    });
                });
            </script>
    </body>
</html>
